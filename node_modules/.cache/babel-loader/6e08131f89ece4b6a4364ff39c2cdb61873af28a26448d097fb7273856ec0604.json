{"ast":null,"code":"import { editAdminAssetsBuy } from \"./api.js\";\nimport assetSelect from './assetSelect.vue';\nexport default {\n  name: \"edit\",\n  components: {\n    assetSelect\n  },\n  props: {\n    data: Object\n  },\n  data() {\n    return {\n      showOwnerWindowFlag: false,\n      submitLoading: false,\n      form: {\n        money: 0,\n        money1: 0,\n        name: \"\",\n        quantity: 0\n      },\n      formValidate: {},\n      selectAssetsCode: \"\"\n    };\n  },\n  methods: {\n    init() {\n      this.handleReset();\n      this.form = this.data;\n    },\n    openAssetSelectWindow() {\n      this.showOwnerWindowFlag = true;\n    },\n    ownerSubmited(e) {\n      this.form.name = e.name;\n      this.selectAssetsCode = e.code;\n      this.form.assetId = e.id;\n      this.showOwnerWindowFlag = false;\n    },\n    handleReset() {\n      this.$refs.form.resetFields();\n    },\n    handleSubmit() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          editAdminAssetsBuy(this.form).then(res => {\n            this.submitLoading = false;\n            if (res.success) {\n              this.$Message.success(\"操作成功\");\n              this.submited();\n            }\n          });\n        }\n      });\n    },\n    close() {\n      this.$emit(\"close\", true);\n    },\n    submited() {\n      this.$emit(\"submited\", true);\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};","map":{"version":3,"names":["editAdminAssetsBuy","assetSelect","name","components","props","data","Object","showOwnerWindowFlag","submitLoading","form","money","money1","quantity","formValidate","selectAssetsCode","methods","init","handleReset","openAssetSelectWindow","ownerSubmited","e","code","assetId","id","$refs","resetFields","handleSubmit","validate","valid","then","res","success","$Message","submited","close","$emit","mounted"],"sources":["src/views/ele/threeAdminAssetsBuy/edit.vue"],"sourcesContent":["<template>\r\n<div>\r\n    <Card>\r\n        <div